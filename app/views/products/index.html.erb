<h1>Products</h1>


<div class="container">
  <div class="row">
    <div class="col-sm-12">
          <table class="table">
           <tr>
             <th>Name</th>
             <th>Url</th>
             <th>Description</th>
           </tr>
            <% @products.each do |product| %>
              <tr>
               <td><%= product.name %></td>
               <td><%= product.url  %></td>
               <td><%= product.description %></td>
              </tr> 
            <% end %>
         </table>
    </div>
  </div>
</div>
